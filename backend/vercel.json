{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "env": {
    "DATABASE_URL": "@database-url",
    "TWITCH_CLIENT_ID": "@twitch-client-id",
    "TWITCH_CLIENT_SECRET": "@twitch-client-secret",
    "JWT_SECRET": "@jwt-secret",
    "CORS_ORIGIN": "@cors-origin"
  }
}